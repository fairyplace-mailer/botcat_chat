{
  "crons": [
    {
      "path": "/api/cron/blob-cleanup",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/finalize-stale-conversations",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/web-kb-seed?maxPages=1500&maxDurationMs=6500",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/web-kb-ingest?limitPages=1",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/rag-ingest",
      "schedule": "20 1 * * *"
    }
  ]
}
